{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@nicknisi/standard/tsconfigs/base",
	"compilerOptions": {
		"outDir": "./dist",
		"lib": ["esnext"],
		"composite": false,
		"noUncheckedIndexedAccess": false,
		"baseUrl": "./",
		"declaration": false,
		"declarationMap": false,
		"useUnknownInCatchVariables": false,
		"types": ["vitest/globals"]
	},
	"include": ["scripts/**/*.ts"]
}
